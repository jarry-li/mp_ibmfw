<%- model_class = ActivityConsume -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:wx_mp_user_id) %>:</strong></dt>
  <dd><%= @activity_consume.wx_mp_user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:activity_id) %>:</strong></dt>
  <dd><%= @activity_consume.activity_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:wx_user_id) %>:</strong></dt>
  <dd><%= @activity_consume.wx_user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:vip_privilege_id) %>:</strong></dt>
  <dd><%= @activity_consume.vip_privilege.try(:title) %></dd>
  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @activity_consume.code %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mobile) %>:</strong></dt>
  <dd><%= @activity_consume.mobile %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @activity_consume.status_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:use_at) %>:</strong></dt>
  <dd><%= @activity_consume.use_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:expire_at) %>:</strong></dt>
  <dd><%= @activity_consume.expire_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @activity_consume.description %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              activity_consumes_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_activity_consume_path(@activity_consume), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              activity_consume_path(@activity_consume),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
